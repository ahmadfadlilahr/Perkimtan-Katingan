APP_NAME="Dinas Perumahan dan Permukiman"
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_TIMEZONE=Asia/Jakarta
APP_URL=https://perkimtan.katingankab.go.id

# CORS Configuration for Production (Optional - sudah ada di config files)
CORS_ALLOWED_ORIGIN_1=https://perkimtan.katingankab.go.id
CORS_ALLOWED_ORIGIN_2=https://www.perkimtan.katingankab.go.id

# Swagger API Documentation
L5_SWAGGER_CONST_HOST=https://perkimtan.katingankab.go.id

APP_LOCALE=id
APP_FALLBACK_LOCALE=id
APP_FAKER_LOCALE=id_ID

LOG_CHANNEL=stack
LOG_STACK=single
LOG_LEVEL=error

# Database Configuration (sesuaikan dengan server produksi)
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=dinas_perkim_production
DB_USERNAME=your_db_user
DB_PASSWORD=your_db_password

# Session & Cache Configuration (Minimal untuk Token-based Auth)
SESSION_DRIVER=file
SESSION_LIFETIME=120
SESSION_ENCRYPT=false

CACHE_STORE=file
QUEUE_CONNECTION=database

# Mail Configuration (sesuaikan dengan SMTP server)
MAIL_MAILER=smtp
MAIL_HOST=smtp.gmail.com
MAIL_PORT=587
MAIL_USERNAME=your_email@gmail.com
MAIL_PASSWORD=your_email_password
MAIL_ENCRYPTION=tls
MAIL_FROM_ADDRESS="noreply@perkimtan.katingankab.go.id"
MAIL_FROM_NAME="${APP_NAME}"

# Google reCAPTCHA Configuration (Jika diperlukan di production)
# RECAPTCHA_SITE_KEY=your_site_key
# RECAPTCHA_SECRET_KEY=your_secret_key

VITE_APP_NAME="${APP_NAME}"
